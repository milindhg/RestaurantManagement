    function changeQuant(event,action){
        
        //alert(action);
        var item = event.target.parentNode;
        var quantity = item.getElementsByTagName('input')[0].value;
        
        //var $button = $j(this);
        //var input= $j(this).parentNode;
        //input.getElementsByTagName('input')[0].readOnly = false;
        //var oldValue = $button.parent().find("input").val();
        //$button.parent().getElementsByTagName('input')[0].readOnly = false;
        //alert(quantity);
        if (action == "inc") {
            item.getElementsByTagName('input')[0].value = parseFloat(quantity) + 1;
        } else {
            if(quantity > 1){
                item.getElementsByTagName('input')[0].value = parseFloat(quantity) - 1;
            }else{
                alert('Quantity can not be less than one');
            }
        } 
        
        
    }
    
